{
  "name": "nyx-solutions/yii2-nyx-validators",
  "description": "Yii2 nyx Validators",
  "type": "library",
  "keywords": ["yii2", "extension", "validators", "forms", "framework", "php"],
  "license": "BSD-3-Clause",
  "version": "4.0.0",
  "authors": [
    {
      "name": "Jonatas Sas",
      "email": "atendimento@jsas.com.br"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.4.0",
    "ext-mbstring": "*",
    "ext-json": "*",
    "nyx-solutions/yii2-nyx": "~2.0.0",
    "nyx-solutions/yii2-nyx-helpers": "~2.0.0",
    "kartik-v/yii2-password": "~1.5.4"
  },
  "autoload": {
    "psr-4": {
      "nyx\\validators\\": "validators/"
    }
  },
  "config": {
    "process-timeout": 1800
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "vcs",
      "url": "https://github.com/nyx-solutions/yii2-nyx-validators"
    }
  ]
}
